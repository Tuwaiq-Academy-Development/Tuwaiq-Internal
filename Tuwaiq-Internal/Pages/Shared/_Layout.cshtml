                    </div>
                    <div class="flex flex-col gap-1">
                        <h1 class="text-3xl font-bold text-secondary">برنامج طويق للتوظيف
                        </h1>
                    </div>

                </div>
                <div>
                    <div class="flex items-center justify-end flex-1">
                        <div class="flex items-center justify-center ">
                            <div x-on:click="openUser = !openUser" class="relative py-3 ml-5 "
                                 :class="{'border-indigo-700 transform transition duration-300 ': openUser }">
                                <div class="flex items-center justify-center space-x-4 cursor-pointer gap-x-4">
                                    <div class=" flex flex-col text-secondary items-center justify-center">
                                        <img src="@User.GetClaim("company_logo")" alt=""
                                             class="object-cover w-12 h-12 border-gray-900 rounded-full shadow border-1">
                                        @User.GetClaim("company_name")
                                    </div>
                                </div>
                                <div x-show="openUser" x-on:click.away="openUser=false" x-transition
                                     class="absolute px-5 py-3 mt-5 bg-white border rounded-lg w-60 left-1 dark:bg-gray-800 dark:border-transparent">
                                    <ul class="flex flex-col gap-2 font-medium dark:text-white">
                                        <li class="border-bottom pb-2 border-gray-200">
                                             مرحبا
                                            @User.GetClaim("name")
                                        </li><li>
                                            <a asp-controller="Account" asp-action="SignOut"
                                               class="hover:text-[#54BBBD]">
                                                تسجيل الخروج
                                            </a>
                                        </li>
                                    </ul>
                                </div>
                            </div>
                        </div>
                    </div>
                </div>
            </div>
        </div>
        <div class="flex flex-1 bg-[#F9FAFB]">
            <div class="hidden md:flex bg-[#F1F2F5] min-h-full py-32 max-w-xs w-full min-w-[20rem]">
                <div class="flex flex-col w-full gap-24 py-5 px-9">
                    <ul class="flex flex-col w-full gap-2">
                        <li>
                            <a asp-page="/" class="flex items-center w-full gap-6 px-6 py-2 rounded-xl"
                                :class="route.url === 'index' ? 'bg-[#E8E8EF]' : 'bg-white'">

                                <h4 class="items-center flex-1 text-lg text-light-gray-700">المرشحين</h4>
                            </a>
                        </li>
                       @if (User.IsInRole("admin"))
                        {
                            <li>
                                <a asp-page="/companies" class="flex items-center w-full gap-6 px-6 py-2 rounded-xl"
                                   :class="route.url === 'companies' ? 'bg-[#E8E8EF]' : 'bg-white'">
                                    <h4 class="items-center flex-1 text-lg text-light-gray-700">الجهات</h4>
                                </a>
                            </li>
                        <li>
                            <a asp-page="/categories" class="flex items-center w-full gap-6 px-6 py-2 rounded-xl"
                                :class="route.url === 'categories' ? 'bg-[#E8E8EF]' : 'bg-white'">

                                <h4 class="items-center flex-1 text-lg text-light-gray-700">التخصصات</h4>
                            </a>
                        </li>
                        }
                    </ul>
                </div>
            </div>
            <div class="w-screen min-h-[80vh] px-8 py-4">
                <div x-show="isLoading" class="flex items-center justify-center h-full my-auto justify-items-center">

                </div>

                <div x-show="!isLoading">
                    @RenderBody()
                </div>
            </div>
        </div>
    </div>
    @* <environment include="Production"> *@
        <script src="~/js/main.js" asp-append-version="true"></script>
    @* </environment> *@

    @await RenderSectionAsync("Scripts", required: false)

